<script>

chart_bar_total_trade = new Highcharts.Chart({
            chart: {
                height:500,
                renderTo: 'market_composition_per_total_value',
                defaultSeriesType: 'bar',
                zoomType :'x'
              //  borderWidth :1,
              //  backgroundColor: "#ECECEC"
            },
            title: {
                text: null
            },
            xAxis: {
                categories:["Mutual Funds","IT Sector","Bank","Pharmaceuticals & Chemicals","Tannery Industries","Engineering","Insurance","Textile","Miscellaneous","Food & Allied","Cement","Fuel & Power","Financial Institutions","Travel & Leisure","Corporate Bond","Telecommunication","Paper & Printing","Services & Real Estate","Ceramics Sector","Jute"],
                title: {
                    text: null
                },
                gridLineWidth :0
            },
            yAxis: {
                min: 0,
                title: {
                    text: 'total_value'
                }
                
            },
            tooltip: {
                formatter: function() {
                    return '<b>'+ this.y +'</b> millions'+' On <b>'+ this.series.name+'</b>';
                }
            },
            plotOptions: {
                bar: {
                    dataLabels: {
                        enabled: true,
                        formatter: function() {
                            return '<b>'+this.y +'</b>';
                        }
                    },
                    lineWidth  :0,
                    groupPadding: 0
                },
                series:
                {
                    pointPadding: 0,
                    groupPadding: 0.1
